  /***************** Boot ****************/
  
  event void Boot.booted() {
    call SerialControl.start();
    debug("App,BOOTED","Booted!\n");

#ifdef NO_DUTYCYCLE_RECEIVER
#warning "NO DUTYCYCLE RECEIVER IS SET"
    if(NO_DUTYCYCLE_RECEIVER==TOS_NODE_ID) {
      call LowPowerListening.setLocalWakeupInterval(0);
    }
#endif
  }
  
  event void SerialControl.startDone(error_t error) {		
    call StartTimer.startOneShot(APPLICATION_START_DELAY);
  }
	
  event void SerialControl.stopDone(error_t err) {}
    
  event void StartTimer.fired() {
    call RadioControl.start();
  }

  event void RadioControl.startDone(error_t error) {
    if(SINGLE_SENDER==0 || SINGLE_SENDER==TOS_NODE_ID) {
      call Timer.startPeriodic(DEFAULT_SAMPLE_INTERVAL);
    }
  }
  
  event void RadioControl.stopDone(error_t err) {}
  	
  /***************** Timer ****************/
  
  event void Timer.fired() { 
    call Sensor.read();
  }

  event void Sensor.readDone(error_t result, uint16_t val) {
    if (dataBusy) {
      debug("App", "Send is busy.\n"); 
    } else {
      data_msg_t* msg = (data_msg_t*) call Send.getPayload(&dataMsg, sizeof(data_msg_t));
      selective_importance_t importance;
      
      counter = (counter+1) % IMPORTANCES_LENGTH;
      
      importance = importances[counter];
      
      msg->value = importance;			
      call SelectivePacket.setImportance(&dataMsg, importance);
      
      signal AppInfo.generated();
      
      if (call Send.send(&dataMsg, sizeof(data_msg_t))==SUCCESS) {
        dataBusy = TRUE;
      }		
    }		
  }
  
  event void Send.sendDone(message_t* message, error_t err) {
    dataBusy = FALSE;

    if(DOUBLE_PACKET_SENDER!=0 && DOUBLE_PACKET_SENDER==TOS_NODE_ID && (counter%2)==1) {
      call Sensor.read();
    }
  }
  
  /***************** Receive ****************/

  event message_t* Receive.receive(message_t* msg, void* payload, uint8_t len) {
    debug("App", "Message received\n");
    return msg;
  }
		   
